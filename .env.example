# Application Configuration
VITE_APP_NAME=VeteranLawAI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://api.veteranlawai.com
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_DOMAIN=auth.veteranlawai.com
VITE_AUTH_CLIENT_ID=your_client_id_here
VITE_AUTH_REDIRECT_URI=https://app.veteranlawai.com/callback

# Google OAuth (for Google Drive integration)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# OCR Service Configuration
VITE_OCR_API_ENDPOINT=https://ocr.veteranlawai.com/api/v1
VITE_OCR_API_KEY=your_ocr_api_key_here

# AI/LLM Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_AI_MODEL=gpt-4-turbo-preview

# Document Storage
VITE_STORAGE_BUCKET=veteranlawai-documents
VITE_STORAGE_REGION=us-east-1

# Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# Feature Flags
VITE_ENABLE_AUDIO_TRANSCRIPTION=true
VITE_ENABLE_CAMERA_OCR=true
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_ANALYTICS=true

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_CSP_REPORT_URI=https://api.veteranlawai.com/csp-report

# Support
VITE_SUPPORT_EMAIL=support@veteranlawai.com
VITE_SUPPORT_PHONE=1-800-VET-LAWS